{
  "plugins": ["jest", "@typescript-eslint", "prettier"],
  "extends": ["plugin:github/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "rules": {
    "i18n-text/no-en": 0,
    "no-console": 0
  },
  "ignorePatterns": ["dist/", "node_modules/", "__tests__"],
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  }
}
